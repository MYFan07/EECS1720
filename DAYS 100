h1{
    text-align: center; /*一级标题居中显示*/
}

span{
    color: blue; /*font color*/
    font-style: italic; 
    font-weight: bold; 
    font-size: 16px; /*font size*/
  
    text-decoration: underline; 
}

p{
    line-height: 30px; /*high*/
}

