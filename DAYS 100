#title{
    font-size: 18px; /*size*/
    font-weight: bold; 
    line-height: 35px; /*high*/
    background-color: #0f7cbf; /*background color*/
    color: white; /*color*/
}

.secondTitle{
    font-size: 14px;
    font-weight: bold;
    line-height: 30px;
    background-color: #e4f1fa;
    color: #0f7cbf;
}
